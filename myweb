pipeline{
    agent any 
    
    environment{
        PATH = "/opt/apache-maven-3.8.5/bin: $PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/aditkumar03/https-github.com-javahometech-myweb'
            }
        }
        stage("maven build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
